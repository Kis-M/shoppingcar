<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mhy.shoppingcar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tubiao"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".LoginRegister.loginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name="com.mhy.shoppingcar.adapter.XuanzuoActivity" />
        <activity android:name="com.mhy.shoppingcar.LoginRegister.RegisterActivity" />
        <activity android:name="com.mhy.shoppingcar.caidan.ArcMenuTestActivity">
            <intent-filter>
                <action android:name="testskippage.intent.action.skip" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="testskippage.intent.category.skip" />
            </intent-filter>
        </activity>
    </application>

</manifest>